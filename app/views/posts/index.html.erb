<h1>Threads</h1>
<% @posts.each do |post | %>

<div class="journal">

  <h4><%= link_to post.title, post %> </h4>
  <p>
  Published <%= time_ago_in_words(post.created_at) %> ago
  by
  <%= post.user.profile_name %>
  </p>
</div>
<% end %>



<div class="digg_pagination">
<%= will_paginate @posts %>
</div>